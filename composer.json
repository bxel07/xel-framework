{
    "name": "xel/framework",
    "description": "Reborn PHP Async Framework",
    "type": "project",
    "authors": [
        {
            "name": "bxel07",
            "email": "yogipratama.ut@gmail.com"
        }
    ],
    "require": {
        "swoole/ide-helper": "~5.0.0",
        "php": "8.2.*",
        "symfony/dotenv": "^7.0",
        "runtime/swoole": "^0.4.0",
        "ext-pdo": "*",
        "psr/container": "^2.0",
      "ext-sockets": "*"
    },
    "autoload": {
        "psr-4": {
            "Devise\\": "devise/",
            "Setup\\" : "setup/",
            "Entity\\" : "entity/",
            "Router\\" : "routes/",
            "Xel\\" : "xel/"
         }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "upscale/swoole-blackfire": "^4.2"
    }
}
